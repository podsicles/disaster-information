<h1>Disaster Information</h1>

<table>
  <thead>
  <td>Title</td>
  <td>Content</td>
  <td>Address</td>
  </thead>
  <% @posts.each do |post| %>
    <tr>
      <td><%= post.title %></td>
      <td><%= post.content %></td>
      <td><%= post.address %></td>
      <td></td>
    </tr>
  <% end %>
</table>